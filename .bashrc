# ~/.bashrc

# set simple blue prompt
export PS1="\e[0;34m\W \$ \e[m"

alias ra="ranger"
alias gs="git status"
alias ga="git add ."
alias gc="git commit -m 'commit'"
alias gp="git push"
alias ls='ls -lhN --color=auto --group-directories-first'
alias lsa='ls -alhN --color=auto --group-directories-first'
alias grep="grep --color=auto"
alias ccat="highlight --out-format=ansi"
alias wifilist="nmcli device wifi list"
alias brianhaller.net="ssh bsh@brianhaller.net"
alias ulanmedia.com="ssh serverpilot@ulanmedia.com"
